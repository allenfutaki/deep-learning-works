EXPERIMENT: "OSNet_par_bces_SGD_lr_0.001_warmup_10_0.01_plateau_10_flip_crop_imagenet"
# [modelname]_[task]_[loss]_[opt]_[lr]_[trick]_[pretrain]

TASK: "par"

MODEL:
  NUM_GPUS: 2
  NUM_CLASSES: 13
  NAME: "osnet"
  PRETRAIN: "own"
  TASK: "classifier"
  SAVE_CRITERION: "loss"

INPUT:
  IMAGE_PAD: 10
  IMAGE_SIZE: (384, 128) #(320, 160)
  IMAGE_CROP_SIZE: (384, 128)
  SIZE_TRAIN: 32
  SIZE_TEST: 256
  PIXEL_MEAN: [0.485, 0.456, 0.406]
  PIXEL_STD: [0.229, 0.224, 0.225]
  
DATALOADER:
  NUM_WORKERS: 1

DATASET:
  NAME: "par"
  TRAIN_PATH: "/path/to/PAR"

TRANSFORM:
  HFLIP: True
  RANDOMCROP: True
  NORMALIZE: True
  RESIZE: True 

SOLVER:
  OPTIMIZER_NAME: "SGD"
  START_EPOCH: 0
  MAX_EPOCHS: 400
  BASE_LR: 0.00035
  BIAS_LR_FACTOR: 1.0
  CENTER_LOSS_LR: 0.5
  CENTER_LOSS_WEIGHT: 0.0005
  WEIGHT_DECAY: 0.0005
  WEIGHT_DECAY_BIAS_FACTOR: 1.0
  EVALUATE_FREQ: 5

  LR_POLICY: "plateau"
  WARMUP: True
  GAMMA: 0.1
  WARMUP_FACTOR: 0.01
  WARMUP_SIZE: 10.0
  PLATEAU_SIZE: 10.0

# osnet_ibn_x1_0_imagenet.pth 

RESUME: "/path/to/osnet_x1_0_imagenet.pth"